{
  "manifest_version": 3,
  "name": "Canvas To Synergy",
  "version": "0.0.3",
  "description": "Download and convert your Canvas grades into Synergy import files.",

  "permissions": [
    "storage"
  ],

  "icons": {
    "16": "images/logo16.png",
    "48": "images/logo48.png",
    "128": "images/logo48.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/lmspps.js"
      ],
      "matches": [
        "https://lms.pps.net/*"
      ]
    },
    {
      "js": [
        "scripts/portlandpublic.js"
      ],
      "matches": [
        "https://portlandpublic.instructure.com/*"
      ]
    },
    {
      "css": [
        "styles/style.css"
      ],
      "js": [
        "scripts/dates.js",
        "scripts/xlsx.js",
        "scripts/read-site.js"
      ],
      "matches": [
        "https://lms.pps.net/*",
        "https://portlandpublic.instructure.com/*"
      ]
    }
  ]
  

}